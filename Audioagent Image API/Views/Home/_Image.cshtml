@model Audioagent_Image_API.Models.ImageModel

<table class="table">
    <tbody>
        <tr>
            <td colspan="2" style="text-align: center; word-break: break-all;">
                <img style="max-width: 100%; max-height: 50vh;" src="@Model.Url" />
            </td>
        </tr>
        <tr><th>Url</th><td><a href="@Model.Url">@Model.Url</a></td></tr>
        <tr><th>Height</th><td>@Model.Height</td></tr>
        <tr><th>Width</th><td>@Model.Width</td></tr>
        @foreach (var item in Model.Metadata)
        {
            <tr>
                <th>@item.Key</th>
                <td>@item.Value</td>
            </tr>
        }
    </tbody>
</table>